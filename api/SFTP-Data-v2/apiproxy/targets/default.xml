      <GoogleAccessToken>
        <Scopes>
          <Scope>https://www.googleapis.com/auth/cloud-platform</Scope>
        </Scopes>
      </GoogleAccessToken>
    </Authentication>
    <URL>https://storage.googleapis.com/storage/v1/b/aip-sftp-cache/o/data-{date}%2F{file}?alt=media</URL>
  </HTTPTargetConnection>
